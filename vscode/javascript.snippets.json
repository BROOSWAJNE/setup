{
	"Log Text": {
		"prefix": "co",
		"body": [
			"console.log(`$1`)$0",
		],
	},
	"Log Object": {
		"prefix": "coo",
		"body": "console.log(`${1:object}`, ${2:${1:object}})$0"
	},
	"Log Object (Highlighted)": {
        "prefix": "cd",
        "body": [
        	"console.log('\\\\x1b[1m\\\\x1b[36m'+",
        	"`${1:description}\\\\n`, ${2:${1:object}},",
        	"'\\\\x1b[0m')$0"
		]
	},
}
