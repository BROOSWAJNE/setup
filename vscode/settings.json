{
    "breadcrumbs.enabled": false,
    "editor.codeActionsOnSave": { "source.fixAll.eslint": true },
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "block",
    "editor.find.cursorMoveOnType": false,
    "editor.foldingHighlight": false,
    "editor.fontFamily": "'Fantasque Sans Mono', 'Droid Sans Mono', 'monospace', monospace",
    "editor.fontSize": 11,
    "editor.minimap.enabled": true,
    "editor.minimap.showSlider": "always",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "selection",
    "editor.scrollBeyondLastLine": false,
    "emmet.includeLanguages": { "javascript": "html" },
    "errorLens.addAnnotationTextPrefixes": false,
    "errorLens.borderRadius": "2px",
    "errorLens.enabledDiagnosticLevels": [ "error", "warning" ],
    "errorLens.fontSize": "0.9em",
    "errorLens.margin": "10ch",
    "errorLens.padding": "2px 1ch 2px",
    "eslint.alwaysShowStatus": true,
    "eslint.validate": [ "javascript", "svelte" ],
    "files.trimTrailingWhitespace": true,
    "files.defaultLanguage": "${activeEditorLanguage}",
    "git.untrackedChanges": "separate",
    "gitlens.blame.format": "${author|20?}: ${message|40?} ${agoOrDate|20-}",
    "gitlens.hovers.currentLine.over": "line",
    "gulp.autoDetect": "off",
    "luminance.tmpdir": "_local/tmp",
    "markdown.extension.toc.updateOnSave": false,
    "search.showLineNumbers": true,
    "window.titleBarStyle": "native",
    "window.zoomLevel": 0,
    "workbench.activityBar.visible": true,
    "workbench.colorTheme": "Arc Dark",
    "workbench.colorCustomizations": {
        "errorLens.errorBackground": "#fff0",
        "errorLens.errorForeground": "#f06464bf",
        "errorLens.errorMessageBackground": "#f064641a",
        "errorLens.warningBackground": "#fff0",
        "errorLens.warningForeground": "#f0b400bf",
        "errorLens.warningMessageBackground": "#f0b4001a",
    },
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.sideBar.location": "right",
    "diffEditor.renderSideBySide": false,
}